{
  "name": "hunchdown-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "setup": "npm install && node ./tools/createEnvironmentFile.js",
    "mongo": "mongod",
    "prod": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run dev\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@feathersjs/errors": "^3.3.0",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.0",
    "ejs": "~2.5.5",
    "errorhandler": "^1.5.0",
    "express": "~4.14.1",
    "express-async-errors": "^2.1.2",
    "express-session": "^1.15.6",
    "method-override": "^2.3.10",
    "mongoose": "^5.0.15",
    "morgan": "~1.7.0",
    "passport": "^0.4.0",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "dotenv": "^5.0.1",
    "jest": "^22.4.3",
    "nodemon": "^1.17.3",
    "supertest": "^3.0.0"
  }
}
