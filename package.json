{
  "name": "hunchdown-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "setup": "npm install && node ./tools/createEnvironmentFile.js",
    "mongo": "mongod",
    "prod": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "start": "npm run dev",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch",
    "mocha": "cross-env NODE_ENV=test mocha test/api/integration --recursive",
    "mocha:watch": "npm run mocha -- --watch"
  },
  "dependencies": {
    "@feathersjs/errors": "^3.3.0",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.0",
    "ejs": "~2.5.5",
    "errorhandler": "^1.5.0",
    "express": "~4.14.1",
    "express-async-errors": "^2.1.2",
    "express-session": "^1.15.6",
    "method-override": "^2.3.10",
    "mongoose": "^5.0.15",
    "mongoose-unique-validator": "^2.0.1",
    "morgan": "~1.7.0",
    "npm": "^6.1.0",
    "passport": "^0.4.0",
    "serve-favicon": "~2.3.2",
    "slug": "^0.9.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "dotenv": "^5.0.1",
    "faker": "^4.1.0",
    "jest": "^22.4.3",
    "mocha": "^5.2.0",
    "mockgoose": "^7.3.5",
    "nodemon": "^1.17.3",
    "sinon": "^6.0.1",
    "sinon-chai": "^3.2.0",
    "sinon-stub-promise": "^4.0.0",
    "supertest": "^3.0.0"
  }
}
